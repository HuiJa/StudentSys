<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.studentSys.dao.ClassesDao">
    <select id="queryClasses" parameterType="int" resultType="Classes">
        SELECT *
        FROM classes
        WHERE xid = #{xid}
    </select>
</mapper>